{
  "name": "microsoft-teams-ext",
  "version": "0.2.0",
  "msteams": {
    "teamsAppId": null
  },
  "private": true,
  "dependencies": {
    "@fluentui/react-components": "^9.15.1",
    "@microsoft/teams-js": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^5.0.1",
    "sinon": "^15.0.1"
  },
  "scripts": {
    "start": "set PORT=3978 HTTP=true && react-scripts start",
